	<name>tiWorldMapPlugin</name>
	<channel>pear.symfony-project.com</channel>
	<summary>A world map plugin</summary>
	<description>A world map plugin</description>
	<lead>
		<name>Pierre FAYOLLE</name>
		<user>pierre42</user>
		<email>pierre@tillid.fr</email>
		<active>yes</active>
	</lead>
	<date>2012-04-19</date>
	<version>
		<release>0.0.1</release>
		<api>0.0.1</api>
	</version>
	<stability>
		<release>beta</release>
		<api>beta</api>
	</stability>
	<license uri="http://www.symfony-project.com/license">
		MIT license
	</license>
	<notes>-</notes>
	<contents>
		<dir name="/">
			<file role="data" name="README" />
			<file role="data" name="README.md" />
			<file role="data" name="LICENSE" />
			<dir name="config">
				<dir name="doctrine">
					<file role="data" name="schema.yml" />
				</dir>
			</dir>
			<dir name="data">
				<dir name="fixtures">
					<file role="data" name="fixtures.yml" />
				</dir>
			</dir>
			<dir name="i18n">
				<dir name="fr">
					<file role="data" name="messages.xml" />
				</dir>
			</dir>
			<dir name="lib">
				<dir name="filter">
					<dir name="doctrine">
						<file role="data" name="PluginCountryFormFilter.class.php" />
					</dir>
				</dir>
				<dir name="form">
					<dir name="doctrine">
						<file role="data" name="PluginCountryForm.class.php" />
					</dir>
				</dir>
				<dir name="model">
					<dir name="doctrine">
						<file role="data" name="PluginCountry.class.php" />
						<file role="data" name="PluginCountryTable.class.php" />
					</dir>
				</dir>
			</dir>
			<dir name="modules">
				<dir name="tiWorldMap">
					<dir name="actions">
						<file role="data" name="actions.class.php" />
						<file role="data" name="components.class.php" />
					</dir>
					<dir name="templates">
						<file role="data" name="_worldmap.php" />
						<file role="data" name="_worldmapjs.php" />
					</dir>
				</dir>
			</dir>
			<dir name="web">
				<dir name="css">
					<file role="data" name="jquery.vector-map.css" />
					<file role="data" name="worldmap.css" />
				</dir>
				<dir name="js">
					<file role="data" name="jquery.vector-map.js" />
				</dir>
			</dir>
		</dir>
	</contents>
	<dependencies>
		<required>
			<php>
				<min>5.0.0</min>
			</php>
			<pearinstaller>
				<min>1.4.1</min>
			</pearinstaller>
			<package>
				<name>symfony</name>
				<channel>pear.symfony-project.com</channel>
				<min>1.3.0</min>
				<max>1.5.0</max>
				<exclude>1.5.0</exclude>
			</package>
		</required>
	</dependencies>
